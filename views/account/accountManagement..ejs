<% if (title) { %>
  <h1><%= title %></h1>
<% } %>

<div class="management-container">
  <%- messages() %>

  <% if (errors) { %>
      <ul class="notice">
          <% errors.array().forEach(error => { %>
              <li><%= error.msg %></li>
          <% }) %>
      </ul>
  <% } %>

  <div class="account-management">
      <p>You're logged in</p>
  </div>
</div>